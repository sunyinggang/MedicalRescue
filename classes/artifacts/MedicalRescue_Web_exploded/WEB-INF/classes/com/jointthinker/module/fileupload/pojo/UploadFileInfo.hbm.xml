<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.jointthinker.module.fileupload.pojo.UploadFileInfo" table="upload_file_info">
		<id name="id" type="java.lang.Long" column="id">
			<generator class="native" />
		</id>
		<property name="uploadtime" type="java.lang.String" column="uploadtime" />
		<property name="category" type="java.lang.String" column="category" />
		<property name="categoryid" type="java.lang.Long" column="categoryid" />
		<property name="username" type="java.lang.String" column="username" />
		<property name="userid" type="java.lang.Long" column="userid" />
		<property name="srcfilename" type="java.lang.String" column="srcfilename" />
		<property name="newfilename" type="java.lang.String" column="newfilename" />
		<property name="filepath" type="java.lang.String" column="filepath" />
	</class>
</hibernate-mapping>